{
  "name": "PetSync",
  "description": "An Android application for tracking pet care",
  "version": "1.0.0",
  "scripts": {
    "simulate": "cd C:\\Users\\%USERNAME%\\AppData\\Local\\Android\\Sdk\\tools && emulator -avd Galaxy_S7_API_23",
    "start": "react-native run-android",
    "devtools": "react-devtools",
    "watch": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "react": "16.4.1",
    "react-native": "^0.55.4"
  },
  "devDependencies": {
    "babel-jest": "23.4.2",
    "babel-preset-react-native": "^4.0.0",
    "buffer": "^5.2.0",
    "enzyme": "^3.4.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "formik": "^1.0.3",
    "immutable": "^3.8.2",
    "jest": "23.4.2",
    "jest-transform-stub": "^1.0.0",
    "native-base": "^2.7.2",
    "react-addons-test-utils": "^15.6.2",
    "react-devtools": "^3.2.3",
    "react-dom": "^16.4.2",
    "react-native-animatable": "^1.3.0",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-svg": "^6.5.2",
    "react-native-uuid": "^1.4.9",
    "react-navigation": "^2.11.2",
    "react-redux": "^5.0.7",
    "react-test-renderer": "16.4.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "styled-components": "^3.4.5",
    "yup": "^0.26.2"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/src/config/tests"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|native-base)"
    ],
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      "styled-components": "<rootDir>/node_modules/styled-components/dist/styled-components.native.cjs.js"
    }
  }
}
